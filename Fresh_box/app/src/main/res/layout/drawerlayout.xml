<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <android.support.v7.widget.Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:background="#e23e9e14"
        android:id="@+id/toolbar"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:title="Fresh Box"/>

        <android.support.v4.widget.DrawerLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:id="@+id/drawerLayout">

    <FrameLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/containerView">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:cardView="http://schemas.android.com/apk/res-auto"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#e3e3e3"
            android:padding="2dp"
            tools:context=".MainActivity">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:weightSum="3">

                    <android.support.v7.widget.CardView
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        cardView:cardBackgroundColor="#ffffff"
                        app:cardElevation="2dp"
                        android:layout_margin="2dp"
                        android:id="@+id/card_view1"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_gravity="center"
                        android:layout_alignParentTop="true">

                    <!--<android.support.v4.widget.SwipeRefreshLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:tools="http://schemas.android.com/tools"
                        android:id="@+id/swipe"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">-->

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:orientation="vertical">
                            <RelativeLayout
                                android:id="@+id/rl"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                            <Gallery
                                android:id="@+id/gallery1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                 />
                            </RelativeLayout>

                        </LinearLayout>
                        <!--</android.support.v4.widget.SwipeRefreshLayout>-->

                    </android.support.v7.widget.CardView>



                    <android.support.v7.widget.CardView
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        cardView:cardBackgroundColor="#ffffff"
                        app:cardElevation="4dp"
                        android:layout_margin="2dp"
                        android:id="@+id/card_view"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        >


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentEnd="true">


                            <LinearLayout
                                android:id="@+id/ln"
                                android:orientation="vertical"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">




                                <LinearLayout
                                    android:weightSum="2"
                                    android:layout_weight="1"
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <LinearLayout
                                        android:layout_width="140dp"
                                        android:layout_height="160dp"
                                        android:layout_weight="1"
                                        android:layout_row="0"
                                        android:weightSum="3"
                                        android:id="@+id/water"
                                        android:orientation="vertical"
                                        android:layout_column="1"

                                        android:layout_marginTop="5dp">

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="110dp"
                                            android:id="@+id/im_subject"
                                            android:layout_weight="1.8"
android:src="@drawable/veg"
                                            android:layout_gravity="center"

                                            />
                                        <TextView

                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:gravity="top|center"
                                            android:text="Vegetables"
                                            android:textColor="#e23e9e14"
                                            android:shadowColor="#ff9e9e9e"
                                            android:shadowDx="1"
                                            android:textStyle="bold"
                                            android:shadowDy="1"
                                            android:shadowRadius="2"
                                            android:layout_weight="1.2"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content" />


                                    </LinearLayout>

                                    <View
                                        android:layout_width="4dp"
                                        android:layout_height="match_parent"
                                        android:background="#e3e3e3"
                                        />

                                    <LinearLayout
                                        android:layout_width="140dp"
                                        android:layout_height="160dp"
                                        android:layout_row="0"
                                        android:layout_weight="1"
                                        android:weightSum="3"
                                        android:id="@+id/food"
                                        android:orientation="vertical"
                                        android:layout_column="2"

                                        android:layout_marginTop="5dp">

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="110dp"
                                            android:layout_weight="1.8"
                                            android:id="@+id/im_quep"
                                            android:src="@drawable/frrr"
                                            android:layout_gravity="center"
                                           />
                                        <TextView
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                        android:gravity="top|center"
android:text="Fruits"
                                            android:textStyle="bold"
                                        android:textColor="#e23e9e14"
                                        android:shadowColor="#ff9e9e9e"
                                        android:shadowDx="1"
                                        android:shadowDy="1"
                                        android:shadowRadius="2"
                                        android:layout_weight="1.2"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content" />

                                    </LinearLayout>

                                </LinearLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="4dp"
                                    android:background="#e3e3e3"
                                    />

                                <LinearLayout
                                    android:weightSum="2"
                                    android:layout_weight="1"
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <LinearLayout
                                        android:layout_width="140dp"
                                        android:layout_height="160dp"
                                        android:layout_weight="1"
                                        android:layout_row="0"
                                        android:id="@+id/coffee"
                                        android:weightSum="3"
                                        android:orientation="vertical"
                                        android:layout_column="1"

                                        android:layout_marginTop="5dp">

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="110dp"
                                            android:id="@+id/im_noti"
                                            android:layout_weight="1.8"
                                            android:src="@drawable/leafy"
                                            android:layout_gravity="center"

                                            />

                                        <TextView

                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:gravity="top|center"
android:text="Leafy"
                                            android:textColor="#e23e9e14"
                                            android:textStyle="bold"
                                            android:shadowColor="#ff9e9e9e"
                                            android:shadowDx="1"
                                            android:shadowDy="1"
                                            android:shadowRadius="2"
                                            android:layout_weight="1.2"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content" />

                                    </LinearLayout>

                                    <View
                                        android:layout_width="4dp"
                                        android:layout_height="match_parent"
                                        android:background="#e3e3e3"
                                        />

                                    <LinearLayout
                                        android:layout_width="140dp"
                                        android:layout_height="160dp"
                                        android:layout_row="0"
                                        android:layout_weight="1"
                                        android:weightSum="3"
                                        android:id="@+id/about"
                                        android:orientation="vertical"
                                        android:layout_column="2"

                                        android:layout_marginTop="5dp">

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="110dp"
                                            android:layout_weight="1.8"
                                            android:id="@+id/im_howstudy"
                                            android:src="@drawable/veg"
                                            android:layout_gravity="center"

                                            />

                                        <TextView
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:gravity="top|center"
                                            android:text="Exostics"
                                            android:textStyle="bold"
                                            android:textColor="#e23e9e14"
                                            android:shadowColor="#ff9e9e9e"
                                            android:shadowDx="1"
                                            android:shadowDy="1"
                                            android:shadowRadius="2"
                                            android:layout_weight="1.2"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content" />

                                    </LinearLayout>

                                </LinearLayout>











                            </LinearLayout>


                            <Button

                                android:id="@+id/place_od"
                                android:layout_width="match_parent"
                                android:layout_height="70dp"
                                android:text="Place Order"
                                android:textStyle="bold"
                                android:textColor="#ffffff"
                                android:background="#e23e9e14"
                                android:layout_below="@+id/ln"
                                />


                        </RelativeLayout>
                    </android.support.v7.widget.CardView>

                    <!--<Button
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:text="Click Here"
                        android:background="@color/nliveo_blue_colorPrimary"
                        android:textColor="@android:color/white"
                        android:id="@+id/btn_test"/>-->

                </LinearLayout>

            </ScrollView>


        </RelativeLayout>

    </FrameLayout>

    <!--<RelativeLayout
        android:id="@+id/container_help"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#88666666"
        android:visibility="invisible" >

        <ImageView
            android:id="@+id/image_help"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/help_screen"
            android:clickable="false"
            android:contentDescription="@string/help_screen"
            android:scaleType="center" />

    </RelativeLayout>-->

    <android.support.design.widget.NavigationView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:id="@+id/shitstuff"

        app:headerLayout="@layout/nav_header_main"
        app:menu="@menu/drawermenu"
        android:layout_marginTop="-2dp" />

</android.support.v4.widget.DrawerLayout>
</LinearLayout>