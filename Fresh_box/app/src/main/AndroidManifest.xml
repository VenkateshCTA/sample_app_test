<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.fresh_box">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Flash_activity"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Register"></activity>
        <activity android:name=".Home"></activity>

        <activity android:name=".Veg_product"></activity>




        <activity
            android:name=".veg_product_list"
            android:parentActivityName=".Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>

        <activity
            android:name=".fruits_products_list"
            android:parentActivityName=".Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>

        <activity
            android:name=".levfy_product_list"
            android:parentActivityName=".Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>

        <activity
            android:name=".exostic_product_list"
            android:parentActivityName=".Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>

        <activity android:name=".Review_order"></activity>
        <activity android:name=".CatalogActivity"></activity>
        <activity android:name=".ShoppingCartActivity"></activity>
        <activity android:name=".ProductDetailsActivity"></activity>


        <activity android:name=".frut_CatalogActivity"></activity>
        <activity android:name=".frut_ShoppingCartActivity"></activity>
        <activity android:name=".frut_ProductDetailsActivity"></activity>



        <activity android:name=".leafy_CatalogActivity"></activity>
        <activity android:name=".leafy_ShoppingCartActivity"></activity>
        <activity android:name=".leafy_ProductDetailsActivity"></activity>



        <activity android:name=".ex_CatalogActivity"></activity>
        <activity android:name=".ex_ShoppingCartActivity"></activity>
        <activity android:name=".ex_ProductDetailsActivity"></activity>


        <activity android:name=".veg_boxx"></activity>
    </application>


</manifest>